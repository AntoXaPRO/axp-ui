{
	"name": "axp-ui",
	"descriiption": "My helper ui lib",
	"version": "1.5.16",
	"homepage": "https://antoxahub.ru/antoxa/axp-ui",
	"repository": {
		"type": "git",
		"url": "https://antoxahub.ru/antoxa/axp-ui.git"
	},
